<%= simple_form_for(@applicant_field_of_study) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Field_Of_Study %>
    <%= f.input :Start_Date %>
    <%= f.input :End_date %>
    <%= f.association :applicant_university , :label_method =>  lambda { |university| "#{university.University_Name}"  }%>
    <%= f.association :applicant_qual, :label_method =>  lambda { |qual| "#{qual.Qualification_Name}"  } %>
    <%= f.association :applicant_qual_status, :label_method =>  lambda { |qual_status| "#{qual_status.Qualification_Status}"  } %>
    <br><br>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: 'btn btn-primary btn-block' %><br>
  </div>
<% end %>
