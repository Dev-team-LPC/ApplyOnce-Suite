<p id="notice"><%= notice %></p>

<h1>Applicant Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Job name</th>
      <th>Company name</th>
      <th>Duties</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Applicant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applicant_experiences.each do |applicant_experience| %>
      <tr>
        <td><%= applicant_experience.Job_Name %></td>
        <td><%= applicant_experience.Company_Name %></td>
        <td><%= applicant_experience.Duties %></td>
        <td><%= applicant_experience.Start_Date %></td>
        <td><%= applicant_experience.End_Date %></td>
        <td><%= applicant_experience.applicant %></td>
        <td><%= link_to 'Show', applicant_experience %></td>
        <td><%= link_to 'Edit', edit_applicant_experience_path(applicant_experience) %></td>
        <td><%= link_to 'Destroy', applicant_experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Applicant Experience', new_applicant_experience_path %>
