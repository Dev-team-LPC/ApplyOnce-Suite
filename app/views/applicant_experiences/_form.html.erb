<%= simple_form_for @applicant_experience do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Job_Name %>
    <%= f.input :Company_Name %>
    <%= f.input :Duties %>
    <label>Employment Type</label><br> 
    <%= f.collection_select(:applicant_employment_type_id, ApplicantEmploymentType.all, :id, :Employment_Type, {:include_blank=>true}, {:class=>'form-control'}) %>
    <%= f.input :Start_Date %>
    <%= f.input :End_Date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: "btn btn-block btn-primary" %><br>
  </div>
<% end %>
