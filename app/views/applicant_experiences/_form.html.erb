<%= simple_form_for @applicant_experience do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Job_Name %>
    <%= f.input :Company_Name %>
    <%= f.input :Duties %>

    <%= f.association :applicant_employment_type, :label_method => lambda { |employment| "#{employment.Employment_Type}" } %>
    <%= f.input :Start_Date %>
    <%= f.input :End_Date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit", class: "btn btn-block btn-primary" %><br>
  </div>
<% end %>
