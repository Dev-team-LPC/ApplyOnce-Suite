<%= simple_form_for(@applicant_experience) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Job_Name %>
    <%= f.input :Company_Name %>
    <%= f.input :Duties %>
    <%= f.input :Start_Date %>
    <%= f.input :End_Date %>
    <%= f.association :applicant %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
